{
  "name": "teamiken/contao-fotoware-api-bundle",
  "type": "contao-bundle",
  "description": "Fotoware api for custom Contao 4 bundles",
  "homepage": "https://contao.org",
  "license": "LGPL-3.0-or-later",
  "authors": [{
    "name": "teamiken",
    "homepage": "https://github.com/teamiken"
  }],
  "require": {
    "php": "^7.4",
    "contao/core-bundle": "4.9.*",
    "guzzlehttp/guzzle": "^7.2",
    "ext-json": "*"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0",
    "contao/test-case": "^4.3",
    "phpunit/phpunit": "^9",
    "symfony/browser-kit": "4.4.* || 5.2.*",
    "symfony/phpunit-bridge": "^5.2"
  },
  "extra": {
    "contao-manager-plugin": "teamiken\\Fotoware\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "teamiken\\Fotoware\\": "src/"
    }
  },
  "support": {
    "issues": "https://github.com/teamiken/contao-fotoware-api/issues",
    "source": "https://github.com/teamiken/contao-fotoware-api"
  },
  "scripts": {
    "cs-fixer": [
      "vendor/bin/ecs check src/ tests/ --fix --ansi"
    ]
  }
}
